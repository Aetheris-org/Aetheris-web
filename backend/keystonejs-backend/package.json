{
  "name": "keystonejs-backend",
  "version": "0.1.0",
  "description": "KeystoneJS backend with GraphQL API for Aetheris platform",
  "main": "dist/index.js",
  "scripts": {
    "dev": "keystone dev",
    "prebuild": "node scripts/fix-next-config.js || true",
    "build": "keystone build",
    "postbuild": "node scripts/fix-next-config.js || true && node scripts/generate-prisma-client.js || true && node scripts/run-migrations.js || true",
    "start": "node scripts/generate-keystone-prisma.js && node scripts/run-migrations.js || true && keystone start",
    "postinstall": "(patch-package || true)",
    "prisma:generate": "npx prisma generate --schema=./schema.prisma",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio"
  },
  "keywords": [
    "keystonejs",
    "graphql",
    "backend"
  ],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=18.0.0 <=22.x.x",
    "npm": ">=6.0.0"
  },
  "dependencies": {
    "@keystone-6/auth": "^8.1.0",
    "@keystone-6/core": "^6.5.1",
    "@keystone-6/fields-document": "^9.1.1",
    "@prisma/client": "^6.19.0",
    "bcryptjs": "^3.0.3",
    "better-sqlite3": "^12.4.1",
    "compression": "^1.8.1",
    "connect-redis": "^9.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express-rate-limit": "^8.2.1",
    "express-session": "^1.18.2",
    "express-validator": "^7.3.0",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "patch-package": "^8.0.1",
    "pg": "^8.16.3",
    "prisma": "^6.19.0",
    "@supabase/supabase-js": "^2.39.3",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/express-session": "^1.18.2",
    "@types/hpp": "^0.2.7",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.1",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/pg": "^8.15.6",
    "nodemon": "^3.1.11",
    "postinstall-postinstall": "^2.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
