// Prisma schema для KeystoneJS
// KeystoneJS использует Prisma под капотом для работы с базой данных

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

// Примечание: KeystoneJS автоматически создаст таблицы на основе схем в keystone.ts
// Эта схема используется для миграций и генерации Prisma Client

